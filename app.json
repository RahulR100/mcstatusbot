
{
    "name": "mcstatusbot",
    "description": "discrod bot for server status",
    "repository": "https://github.com/Huskydog9988/mcstatusbot/",
    "env": {
      "token": {
        "description": "Your discord bot's token.",
        "required": true
      },
      "ip": {
        "description": "Main mc server's ip.",
        "required": true
      },
      "port": {
        "description": "Main mc server's port, if none is set it defaults to '25566'.",
        "required": false
      },
      "prefix": {
        "description": "Your discord bot's prefix, if none then defaults to '!'.",
        "required": false
      }
    }
}